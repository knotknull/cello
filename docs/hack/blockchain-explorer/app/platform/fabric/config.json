{
  "network-configs": {
    "ccp-network": {
      "version": "1.0",
      "clients": {
        "client-1": {
          "tlsEnable": true,
          "organization": "ccp",
          "channel": "firstchannel",
          "credentialStore": {
            "path": "./tmp/credentialStore_Org1/credential",
            "cryptoStore": {
              "path": "./tmp/credentialStore_Org1/crypto"
            }
          }
        }
      },
      "channels": {
        "firstchannel": {
          "peers": {
            "peer1-ccp": {}
          },
          "connection": {
            "timeout": {
              "peer": {
                "endorser": "18000",
                "eventHub": "18000",
                "eventReg": "18000"
              }
            }
          }
        }
      },
      "organizations": {
        "ccp": {
          "mspid": "ccp",
          "fullpath": false,
          "adminPrivateKey": {
            "path":
              "/opt/fabric/hack_fab_bc/run/keyfiles/ccp/users/Admin@ccp/msp/keystore/"
          },

          "signedCert": {
            "path":
              "/opt/fabric/hack_fab_bc/run/keyfiles/ccp/users/Admin@ccp/msp/signcerts/"
          }
        },
        "clnt1": {
          "mspid": "clnt1",
          "adminPrivateKey": {
            "path":
              "/opt/fabric/hack_fab_bc/run/keyfiles/clnt1/users/Admin@clnt1/msp/keystore/"
          }
        },
        "dlr1": {
          "mspid": "dlr1",
          "adminPrivateKey": {
            "path":
              "/opt/fabric/hack_fab_bc/run/keyfiles/dlr1/users/Admin@dlr1/msp/keystore/"
          }
        },
        "ord": {
          "mspid": "ord",
          "adminPrivateKey": {
            "path":
              "/opt/fabric/hack_fab_bc/run/keyfiles/ord/users/Admin@ord/msp/keystore/"
          }
        }
      },
      "peers": {
        "peer1-ccp": {
          "tlsCACerts": {
            "path":
	      "/opt/fabric/hack_fab_bc/run/keyfiles/ccp/peers/peer1-ccp.ccp/tls/ca.crt"
          },
          "url": "grpcs://peer1-ccp:7051",
          "eventUrl": "grpcs://peer1-ccp:7053",
          "grpcOptions": {
            "ssl-target-name-override": "peer1-ccp"
          }
        },
        "peer1-clnt1": {
          "url": "grpcs://peer1-clnt1:7051"
        },
        "peer1-dlr1": {
          "url": "grpcs://peer1-dlr1:7051"
        }
      },
      "orderers": {
        "orderer1-ord": {
          "url": "grpcs://orderer1-ord:7050"
        }
      }
    },
    "network-2": {}
  },
  "configtxgenToolPath": "/opt/fabric/bin",
  "license": "Apache-2.0"
}
